Full_Bell_state = Circuit()
for q_0,q_1 in tested_qubits:
    q_l_int = int(min(q_0,q_1))
    q_b_int = int(max(q_0,q_1))
    Full_Bell_state = Full_Bell_state.h(q_l_int).cnot(q_l_int, q_b_int)

# print(Full_Bell_state)


Full_Bell_state_task = Ankaa_device.run(Full_Bell_state, shots=4096, disable_qubit_rewiring=True)


