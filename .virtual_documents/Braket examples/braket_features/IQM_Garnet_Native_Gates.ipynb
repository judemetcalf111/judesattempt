








import numpy as np
import sympy as sp
from sympy import nsimplify

from braket.aws import AwsDevice
from braket.circuits import Circuit

pi = np.pi





def rx_symbolic(x):
    return sp.Matrix([[sp.cos(x / 2), -1j * sp.sin(x / 2)], [-1j * sp.sin(x / 2), sp.cos(x / 2)]])


def rz_symbolic(x):
    return sp.Matrix([[sp.exp(-1j * x / 2), 0], [0, sp.exp(1j * x / 2)]])


def prx_symbolic(a1, a2):
    return rz_symbolic(a2) * rx_symbolic(a1) * rz_symbolic(-a2)





theta, phi = sp.symbols("theta phi")
prx_matrix = prx_symbolic(theta, phi)

nsimplify(prx_matrix, tolerance=1e-8)





theta, phi = sp.symbols("theta phi")
rx_matrix = prx_symbolic(theta, 0)

nsimplify(rx_matrix.evalf(), tolerance=1e-8)





theta, phi = sp.symbols("theta phi")
ry_matrix = prx_symbolic(theta, pi / 2)

nsimplify(ry_matrix.evalf(), tolerance=1e-8)





